SELECT b.nim, a.statushadir
FROM akademik.ak_absensimhs a 
 LEFT OUTER JOIN (
	
     )b ON a.nim = b.nim
WHERE a.idjadwal = 262336;

SELECT * FROM akademik.ak_krs 
	INNER JOIN akademik.ak_kelas 
	ON ak_krs.idkelas = ak_kelas.idkelas 
	WHERE ak_kelas.idkelas='197333'


